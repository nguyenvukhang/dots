cmd - 1 : ~/dots/mac/.config/yabai/spaces.py 1 --focus
cmd - 2 : ~/dots/mac/.config/yabai/spaces.py 2 --focus
cmd - 3 : ~/dots/mac/.config/yabai/spaces.py 3 --focus
cmd - 4 : ~/dots/mac/.config/yabai/spaces.py 4 --focus
cmd - 5 : ~/dots/mac/.config/yabai/spaces.py 5 --focus

cmd + shift - 1 : ~/dots/mac/.config/yabai/spaces.py 1 --move
cmd + shift - 2 : ~/dots/mac/.config/yabai/spaces.py 2 --move
cmd + shift - 3 : ~/dots/mac/.config/yabai/spaces.py 3 --move
cmd + shift - 4 : ~/dots/mac/.config/yabai/spaces.py 4 --move
cmd + shift - 5 : ~/dots/mac/.config/yabai/spaces.py 5 --move

cmd - 0x32 : open -a Mission\ Control.app

cmd - 0x24 : open -a kitty.app -n

cmd - up : yabai -m window --focus prev
cmd - down : yabai -m window --focus next

cmd + shift - up : yabai -m window --swap prev
cmd + shift - down : yabai -m window --swap next

cmd + shift - 0x2F : yabai -m window --display next
cmd + shift - 0x2B : yabai -m window --display prev
cmd + shift + ctrl + alt - a : yabai -m window --display prev
cmd + shift + ctrl + alt - s : yabai -m window --display next
