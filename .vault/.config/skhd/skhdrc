cmd - 1 : ~/dots/mac/.config/yabai/spaces.py 1 --focus
cmd - 2 : ~/dots/mac/.config/yabai/spaces.py 2 --focus
cmd - 3 : ~/dots/mac/.config/yabai/spaces.py 3 --focus
cmd - 4 : ~/dots/mac/.config/yabai/spaces.py 4 --focus
cmd - 5 : ~/dots/mac/.config/yabai/spaces.py 5 --focus

cmd + shift - 1 : ~/dots/mac/.config/yabai/spaces.py 1 --move
cmd + shift - 2 : ~/dots/mac/.config/yabai/spaces.py 2 --move
cmd + shift - 3 : ~/dots/mac/.config/yabai/spaces.py 3 --move
cmd + shift - 4 : ~/dots/mac/.config/yabai/spaces.py 4 --move
cmd + shift - 5 : ~/dots/mac/.config/yabai/spaces.py 5 --move

# 0x32 = backtick
cmd - 0x32 : open -a Mission\ Control.app
cmd + shift - 0x32 : open -a Launchpad.app
cmd - return : open -a kitty.app -n
cmd + ctrl + alt + shift - s : open -a Spotify

# no-shift -> change focus
# shift -> movements

cmd - left          : yabai -m window  --focus prev
cmd - right         : yabai -m window  --focus next
cmd - up            : yabai -m display --focus next
cmd - down          : yabai -m display --focus prev

cmd + shift - left  : yabai -m window  --swap prev
cmd + shift - right : yabai -m window  --swap next
cmd + shift - up    : yabai -m window  --display next; yabai -m display --focus next
cmd + shift - down  : yabai -m window  --display prev; yabai -m display --focus prev

shift - f8 : yabai -m space --layout bsp
shift - f9 : yabai -m space --layout stack

# vim:ft=yaml
