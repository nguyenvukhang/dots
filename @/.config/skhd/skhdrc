# drop ctrl because ctrl + click gives right click
cmd + alt + shift - c : ~/dots/intuous/bin/intuous cycle
cmd + alt + shift - 3 : ~/dots/intuous/bin/intuous zoom-out
cmd + alt + shift - 4 : ~/dots/intuous/bin/intuous zoom-in

# period .
cmd + alt + shift - 0x2F : ~/dots/intuous/bin/intuous load-preset ~/dots/intuous/presets/90-centered.prefs
